{
  "name": "moose-player",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Michel Kraemer",
  "description": "A simplistic console music player",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=8"
  },
  "files": [
    "index.js",
    "ui.js",
    "reindex.js"
  ],
  "keywords": [
    "audio",
    "player",
    "music",
    "terminal",
    "command-line",
    "console",
    "cli",
    "ffmpeg",
    "libav",
    "libsdl2",
    "sdl",
    "sdl2",
    "shell",
    "text",
    "xterm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/michel-kraemer/moose-player.git"
  },
  "bugs": "https://github.com/michel-kraemer/moose-player/issues",
  "dependencies": {
    "ansi-escapes": "^3.1.0",
    "chalk": "^2.4.1",
    "lunr": "^2.3.3",
    "music-metadata": "^3.1.2",
    "native-player": "./native-player",
    "rmdir": "^1.2.0"
  },
  "scripts": {
    "test": "xo"
  },
  "devDependencies": {
    "xo": "^0.23.0"
  },
  "xo": {
    "rules": {
      "camelcase": [
        "off"
      ],
      "capitalized-comments": [
        "error",
        "never"
      ],
      "import/order": [
        "error",
        {
          "newlines-between": "always"
        }
      ],
      "no-await-in-loop": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double"
      ]
    },
    "space": true
  }
}
